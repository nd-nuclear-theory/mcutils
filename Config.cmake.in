@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

get_target_property(
  @PROJECT_NAME@_INTERFACE_LINK_LIBRARIES @PROJECT_NAME@::@PROJECT_NAME@ INTERFACE_LINK_LIBRARIES
)

find_dependency(am)
if(Eigen3::Eigen IN_LIST @PROJECT_NAME@_INTERFACE_LINK_LIBRARIES)
  find_dependency(Eigen3 REQUIRED NO_MODULE)
endif()
if(fmt::fmt IN_LIST @PROJECT_NAME@_INTERFACE_LINK_LIBRARIES)
  find_dependency(fmt)
endif()
if(GSL::gsl IN_LIST @PROJECT_NAME@_INTERFACE_LINK_LIBRARIES)
  find_dependency(GSL)
endif()
